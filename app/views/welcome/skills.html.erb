<body>
    
    <!-- Content Section -->
    <section>
        <div class="container">
            <div class="row">
                <%= link_to image_tag("logo.png", alt: "Dental Clinic", height: "350px", width: "400px"), "#", id: "pic" %>

                <div class="col-lg-12">
                    <h1 class="section-heading">Scientist Kids</h1>
                    <p class="lead section-lead">Programming Languages used: HTML, CSS, Javascript, SQL</p>
                    <p class="section-paragraph">A simple web application that would help young students learn about Science.</p>
                </div>
            </div>
        </div>
    </section>

    <!-- Content Section -->
    <section>
        <div class="container">
            <div class="row">
                <%= link_to image_tag("logo_1.png", alt: "Dental Clinic", height: "350px", width: "400px"), "#", id: "pic" %>

                <div class="col-lg-12">
                    <h1 class="section-heading">Dental Clinic System</h1>
                    <p class="lead section-lead">Programming Language used: Java</p>
                    <p class="section-paragraph">A Java application that would help the dentists, secretary, and customers alike to schedule appointments.</p>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
    </section>

    <!-- Footer -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; Chryss Flores Website 2017</p>
                </div>
            </div>
            <!-- /.row -->
        </div>
        <!-- /.container -->
